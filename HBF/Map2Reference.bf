alignOptions = {};alignOptions ["SEQ_ALIGN_CHARACTER_MAP"]="ACGT";scoreMatrix = {{5,-1,-4,-4}{-4,5,-4,-4}{-4,-4,5,-4}{-4,-4,-4,5}};alignOptions ["SEQ_ALIGN_SCORE_MATRIX"] = 	scoreMatrix;alignOptions ["SEQ_ALIGN_GAP_OPEN"]		= 	10;alignOptions ["SEQ_ALIGN_GAP_OPEN2"]	= 	10;alignOptions ["SEQ_ALIGN_GAP_EXTEND"]	= 	1;alignOptions ["SEQ_ALIGN_GAP_EXTEND2"]	= 	1;alignOptions ["SEQ_ALIGN_AFFINE"]		=   1;alignOptions ["SEQ_ALIGN_NO_TP"]		=   1;HXB_FULL_LENGTH 		   = ""&&1;function  setupSequenceMap (ref,query){	charCount	= Abs(query);	sequenceMap = {charCount,1};		inStr 		 = {{ref,query}};	AlignSequences (aligned, inStr, alignOptions);	s1 = (aligned[0])[1];	s2 = (aligned[0])[2];	al = Abs(s2);		coord1   = 0;	coord2   = 0;	for (position = 0; position < al; position = position + 1)	{		if (query[coord2] == s2[position])		{			sequenceMap[coord2] = coord1;			if (s1[position] != "-")			{				coord1 = coord1 + 1;			}			coord2 = coord2 + 1;		}		else		{			coord1 = coord1 + 1;		}	}	for (; coord2 < charCount; coord2 = coord2 + 1)	{		sequenceMap[coord2] = Abs(ref);	}	return      sequenceMap;}function mapFromFile (){	SetDialogPrompt ("Load results from:");	ExecuteAFile (PROMPT_FOR_FILE);	choices = Rows(screen_results);	whichSequence = 0;	if (Columns(choices)>1)	{		choiceMx = {Columns(choices),2};		for (k = 0; k < Columns(choices); k=k+1)		{			choiceMx [k][0] = choices[k]; 			choiceMx [k][1] = choices[k]; 		}		ChoiceList (whichSequence,"What sequence?",1,SKIP_NONE,choiceMx);		if (whichSequence < 0)		{			return -1;		}	}	sm = setupSequenceMap (HXB_FULL_LENGTH, (screen_results[choices[whichSequence]])["SEQUENCE"]);	bp = (screen_results[choices[whichSequence]])["BREAKPOINTS"];	if (Rows(bp))	{		for (k = 0; k < Rows(bp); k=k+1)		{			fprintf (stdout, "BP ", k+1, " at ", sm[bp[k][0]], " [", sm[bp[k][1]], "-", sm[bp[k][2]], "]\n");		}	}	return 0;}mapFromFile();